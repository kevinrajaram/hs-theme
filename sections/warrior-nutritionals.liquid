<script>
$("div[data-more]").hide().each(function () {
  var open_text = $(this).data("Learn more");
  open_text = typeof open_text !== "undefined" ? open_text : "Learn more";
  $(this).before('<a data-more-toggle href="#" class="more-toggle">' + open_text + "</a>");
});

$("[data-more-toggle]").click(function (e) {
  e.preventDefault();
  $(this).toggleClass("active");
  var open_text = $(this).siblings("div[data-more]").data("open-text");
  var close_text = $(this).siblings("div[data-more]").data("close-text");

  if (typeof open_text == "undefined") {
    open_text = "Learn more";
  }

  if (typeof close_text == "undefined") {
    close_text = "Close";
  }

  if ($(this).text() == open_text) {
    $(this).html(close_text).next("div[data-more]").slideToggle("fast").after(this);
  } else {
    $(this).html(open_text).prev("div[data-more]").slideToggle("fast").before(this);
  }
});
</script>

