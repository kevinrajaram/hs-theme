    function wfxDiscountError(status) {
        if(status === true) {
            $('.wfx-discount-error').show().html(' Discount not found.');
        } else {
            $('.wfx-discount-error').hide().html('');
        }
    }

    function wfxSubmitDiscount() {
        var id = addressId('get');
        discount_code = $('#wfxDiscountCode').val();

        if (discount_total > 0) {
            wfxRemoveDiscount(id).done(function () {
                wfxAddDiscount(id, discount_code).done(function () {
                    wfxSubscription(id, "update");
                    wayfxAlert("success");
                    wfxDiscountError(false);
                }).fail(function () {
                    var discount_original_code = $("body").attr('data-discount-code');
                    wfxAddDiscount(id, discount_original_code).done(function () {
                        wfxSubscription(id, "update");
                        wfxDiscountError(false);
                    }).fail(function () {
                        wfxDiscountError(true);
                    });
                });
            }).fail(function () {
                wfxDiscountError(true);
                return false;
            });
        } else {
            wfxAddDiscount(id, discount_code).done(function () {
                wfxSubscription(id, "update");
                wayfxAlert("success");
                wfxDiscountError(false)
            }).fail(function () {
                wfxDiscountError(true);
            });
        }
    }

