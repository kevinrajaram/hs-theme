{%- assign title_to_remove = '10.00% Off Auto renew' -%}
{%- assign total_products = 0 -%}
{%- assign has_variants = false  -%}
{%- capture recharge_products -%}
    {%- for product in collections['all'].products -%}
        {%- assign total_products = total_products | plus: 1 -%}
        {%- assign product_title = product.title -%}
        {%- assign product_description = product.description -%}
        {%- assign current_variant = product.selected_or_first_available_variant -%}
        {%- assign normal_price = current_variant.price -%}
        {%- assign normal_variant_id = product.variants.first.id -%}
        {%- assign subscription_variant_id = current_variant.id -%}
        {%- assign subscription_id = product.metafields.subscriptions.subscription_id -%}
        {%- assign subscription_price = current_variant.metafields.subscriptions.discount_variant_price | times: 100 -%}
        {%- assign product_image = product | img_url: 'grande' -%}
        {%- if subscription_variant_id and subscription_price > 0 -%}
            <div class="wayfx-product__grid-item" id="wayfx-product-{{ normal_variant_id }}" data-qty="1" data-variant-id="{{ normal_variant_id }}">
                <div class="wayfx-product__grid-image">
                    <img src="{{ product_image }}" alt="{{ product.featured_image.alt | escape }}"/>
                </div>
                <div class="wayfx-product__grid-info">
                    <div class="wayfx-product__grid-reviews">
                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="83" height="14" viewBox="0 0 83 14"><defs><path id="a" d="M0 0h82.833v14H0z"/></defs><g fill="none" fill-rule="evenodd"><mask id="b" fill="#fff"><use xlink:href="#a"/></mask><path fill="#FF9023" fill-rule="nonzero" d="M7.87444233.29870242l1.59554581 4.62387762 4.98531756.05649475c.4289937.00488896.6065849.53995939.262502.79038321l-3.9996865 2.91382514 1.4856612 4.65864366c.1276437.4008954-.3368682.7311723-.6870559.4883536l-4.06683809-2.8231076-4.06739312 2.8225644c-.34963264.2428187-.81469956-.0880015-.68705589-.4883537l1.48566125-4.65864358L.18141418 5.76891478c-.34408292-.25042382-.16649174-.78549424.26250197-.79038321l4.98531755-.05649475L7.02477951.2981592c.13763317-.3976361.71258463-.3976361.84966282.00054322zm17.87906617 0l1.5955458 4.62387762 4.9853176.05649475c.4289937.00488896.6065849.53995939.262502.79038321l-3.9996865 2.91382514 1.4856613 4.65864366c.1276436.4008954-.3368683.7311723-.6870559.4883536l-4.0668382-2.8231076-4.0673931 2.8225644c-.3496326.2428187-.8146996-.0880015-.6870559-.4883537l1.4856613-4.65864358-3.9996865-2.91382514c-.3440829-.25042382-.1664917-.78549424.262502-.79038321l4.9853175-.05649475L24.9038457.2981592c.1376332-.3976361.7125846-.3976361.8496628.00054322zm18.6240273 0l1.5955458 4.62387762 4.9853176.05649475c.4289937.00488896.6065849.53995939.262502.79038321l-3.9996865 2.91382514 1.4856613 4.65864366c.1276436.4008954-.3368683.7311723-.6870559.4883536l-4.0668382-2.8231076-4.0673931 2.8225644c-.3496326.2428187-.8146996-.0880015-.6870559-.4883537l1.4856613-4.65864358-3.9996865-2.91382514c-.3440829-.25042382-.1664917-.78549424.262502-.79038321l4.9853175-.05649475L43.527873.2981592c.1376332-.3976361.7125846-.3976361.8496628.00054322zm17.8790662 0l1.5955459 4.62387762 4.9853175.05649475c.4289937.00488896.6065849.53995939.262502.79038321l-3.9996865 2.91382514 1.4856613 4.65864366c.1276436.4008954-.3368683.7311723-.6870559.48835