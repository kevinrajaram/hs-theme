<script>
    var productImageData = '[{"product_images": { {%- for product in collections['all'].products -%}{%- for variant in product.variants -%}"{{ variant.id }}": "{{ product.featured_image.src | img_url: '400x400' }}",{%- endfor -%}{%- if forloop.last -%}"wayfx": "images"{%- endif -%}{%- endfor -%} }}]';
    var productImageJson = $.parseJSON(productImageData);

    var data,
        current_shipment_date,
        delivery_date,
        delivery_date_formatted,
        delivery_frequency,
        first_name,
        last_name,
        full_name,
        address,
        address_id,
        edit_shipping_url,
        shipping_address,
        street_address_1,
        street_address_2,
        city,
        province,
        zip,
        country,
        billing_address,
        order,
        order_address_id,
        phone,
        product,
        product_title,
        product_id,
        product_address_id,
        product_subscription_id,
        product_onetime_id,
        product_image_url,
        product_quantity,
        product_variant_title,
        product_price,
        product_total_price,
        product_item,
        product_details,
        product_details_container,
        product_next_charge,
        product_updated_next_charge,
        product_charge_id,
        product_status,
        product_type,
        urlPathArray,
        subscription_id,
        subscription_total,
        subscription_name,
        subscription_item,
        discount_total,
        discount_code_current,
        before_discount_total,
        discount_code;

    $(document).ajaxStart(function () {
        wayfxAlert('update');
    });

    $(document).ajaxError(function () {
        wayfxAlert('error');
    });

    $(document).ajaxStop(function () {
        wayfxAlert('done');
    });

    $.fn.datepicker.language['en'] = {
        days: ['Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday'],
        daysShort: ['Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat'],
        daysMin: ['S', 'M', 'T', 'W', 'T', 'F', 'S'],
        months: ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'],
        monthsShort: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'],
        today: 'Today',
        clear: 'Clear',
        dateFormat: 'yyyy-mm-dd',
        timeFormat: 'hh:ii aa',
    }

    function wayfxAlert(status) {
        if(status === 'success') {
            $(wayfx.portalNotification).addClass('active');
        }
        if(status === 'update') {
            $(wayfx.updatingAccount).addClass('active');
            $(wayfx.portalNotification).removeClass('active');
        }
        if(status === 'error') {
            $(wayfx.updatingAccount).removeClass('active');
            $(wayfx.portalNotification).removeClass('active');
        }
        if(status === 'done') {
            $(wayfx.updatingAccount).removeClass('active');
        }
    }

    function addressId(type, id) {
        if(type === 'set') {
            $('body').attr('data-address-id', id);
        } else {
            return $('body').attr('data-address-id');
        }
    }

    function closeSidebar() {
        $(wayfx.index).removeClass('wayfx-drawer-active');
        $(wayfx.drawer).removeClass('active');
        $(wayfx.drawerBlock).hide();
        $(wayfx.swapBlock).hide();
        $(wayfx.drawerNormalBlock).show();
    }

    function cancelSwap() {
        $(wayfx.swapBlock).hide();
        $(wayfx.drawerNormalBlock).fadeIn();
    }

    function swapProduct(t) {
        var $this = $(t);
        var current_subscription = $this.data('current-subscription'),
            swap_product_id = $this.data('variant-id'),
            one_time_id = $(wayfx.drawerProduct).attr("data-product-onetime-id");
        closeSidebar();
        $(wayfx.updatingAccount).addClass('active');
        wayfxSwapProduct(current_subscription, one_time_id, swap_product_id, null);
    }

    function wfxShowSwapProducts() {
        $(wayfx.drawerNormalBlock).hide();
        $(wayfx.swapBlock).fadeIn();
    }

    function wfxRetention() {
        return $.ajax({
            url: '/tools/recurring/portal/' + wayfx.customerHash + '/request_objects?token=' + wayfx.customerToken,
            type: 'get',
            dataType: 'json',
            data: {schema: '{ "retention_strategies": [] }'}
        });
    }

    function wfxAddress() {
        return $.ajax({
            url: '/tools/recurring/portal/'+ wayfx.customerHash +'/request_objects?token=' + wayfx.customerToken,
            type: 'get',
            dataType: 'json',
            data: { schema: '{ "addresses": [] }' },
        });
    }

    function wfxFrequency() {
        return $.ajax({
            url: '/tools/recurring/portal/'+ wayfx.customerHash +'/request_objects?token=' + wayfx.customerToken,
            type: 'get',
            dataType: 'json',
            data: { schema: '{ "products": [] }' },
        });
    }

    function wfxCharges() {
        return $.ajax({
            url: '/tools/recurring/portal/'+ wayfx.customerHash +'/request_objects?token=' + wayfx.customerToken,
            type: 'get',
            dataType: 'json',
            data: { schema: '{ "charges": { "status": "SUCCESS" } }' },
        }).done(function (response) {
            var minimum = parseInt($('body').attr('data-cancellation-minimum'));
            var allow_cancellation = $('body').attr('data-allow-cancellation');

            if(allow_cancellation === 'true') {
                if(response.charges.length >= minimum) {
                    $(wayfx.cancelSubscription).show();
                    $(wayfx.cancelProduct).show();
                }
            }
        });
    }

    function wfxPaymentSource() {
        return $.ajax({
            url: '/tools/recurring/portal/' + wayfx.customerHash + '/request_objects?token=' + wayfx.customerToken,
            type: 'get',
            dataType: 'json',
            data: {schema: '{ "payment_sources": [] }'}
        });
    }

    function wfxBillingDetails() {
        wfxPaymentSource().done(function (response) {
            var payment = response.payment_sources[0],
                payment_address = payment.billing_address,
                payment_method_brand = payment.card_brand,
                payment_method_card_last4 = payment.card_last4,
                payment_method_card_exp_month = payment.card_exp_month,
                payment_method_card_exp_year = payment.card_exp_year,
                payment_type = payment.payment_type,
                first_name = payment_address.first_name,
                last_name = payment_address.last_name,
                street_address_1 = payment_address.address1,
                street_address_2 = payment_address.address2,
                city = payment_address.city,
                province = payment_address.province,
                country = payment_address.country,
                zip = payment_address.zip,
                phone = payment_address.phone,
                full_name = first_name + ' ' + last_name;
            if(street_address_2 === null) {
                street_address_2 = '';
            }
            var billing_address = street_address_1 + ' ' + street_address_2 + '<br />' + city + ', ' + province + ' ' + zip + '<br />' + country + '<br />' + phone;

            $('#billingAddress, #billingAddress2').html(full_name + '<br />' + billing_address).removeClass('loading');

            if (payment_type === 'credit' || payment_type === 'debit') {
                var payment_method_brand_image = '';
                switch (payment_method_brand) {
                    case 'visa':
                        payment_method_brand_image = 'https://ucarecdn.com/b493e387-2385-412f-8569-995ce7d9b506/visa.svg';
                        break;
                    case 'american_express':
                        payment_method_brand_image = 'https://ucarecdn.com/ddb5579f-f16c-4e86-b148-b0c33f627860/amex.svg';
                        break;
                    case 'mastercard':
                        payment_method_brand_image = 'https://ucarecdn.com/9a740cdf-8832-4dc6-8ec1-f89c9268aea2/mastercard.svg';
                        break;
                    case 'discover':
                        payment_method_brand_image = 'https://ucarecdn.com/578a9ba3-77eb-4de4-95c1-265964993a55/discover.svg';
                        break;
                }
                if (payment_method_brand_image === '') {
                    $('#cardType, #cardType2').html(payment_method_brand);
                } else {
                    $('#cardType, #cardType2').html('<img src="' + payment_method_brand_image + '" alt="" />');
                }
                $('#cardLast4, #cardLast42').html('*' + payment_method_card_last4);
                $('#cardMonth, #cardMonth2').html(payment_method_card_exp_month + '/');
                $('#cardYear, #cardYear2').html(payment_method_card_exp_year);
            } else if (payment_type === 'paypal') {
                $('#cardType, #cardType2').html("<img src='https://ucarecdn.com/22112e92-0b61-4017-8fdf-0bbdb8d15c5e/paypal.svg' alt='' />");
            } else {
                $('#cardType, #cardType2').html('Apple Pay');
            }
        });
    }

    function showDiscountInput() {
        $(wayfx.discountBlock).find('.wayfx-block__discount-btn').hide();
        $(wayfx.discountBlock).find('.wayfx-block__discount-form').show();
    }

    function wayfxSwapProduct(subscription, onetime_id, product, qty) {
        var url,
            data;

        if(subscription) {
            url = '/tools/recurring/portal/' + wayfx.customerHash + '/subscriptions/' + subscription + '/swap?token=' + wayfx.customerToken;
        } else {
            url = '/tools/recurring/portal/' + wayfx.customerHash + '/onetimes/' + onetime_id + '?token=' + wayfx.customerToken
        }

        if(qty) {
            data = { "quantity": qty };
        } else {
            data = { "shopify_variant_id": product };
        }

        return $.ajax({
            url: url,
            type: 'post',
            dataType: 'json',
            data: data
        }).done(function (response) {
            var address_id = addressId('get');
            wfxSubscription(address_id, "update");
            wayfxAlert("success");
        })
            .fail(function (response) {
                {%- if enable_dev_mode -%}
                console.log(response);
                {%- endif -%}
                wayfxAlert("error");
            });
    }

    {%- if request.path contains 'payment_sources' or request.path contains 'orders' -%}

    function wfxPortalLoad() {
        var searchParams = new URLSearchParams(window.location.search),
            order_address_id = searchParams.get('address_id');

        if(order_address_id) {
            wfxSubscription(order_address_id, 'update');
        } else {
            wfxAddress().done(function addresses(response) {
                wfxCharges();

                address = response.addresses[0];
                address_id = address.id;
                edit_shipping_url = '<a href="/tools/recurring/portal/' + wayfx.customerHash + '/addresses/' + response.addresses[0].id + '" class="wayfx-btn wayfx-btn--primary">Edit Shipping Address</a>';
                $(wayfx.editShippingUrl).html(edit_shipping_url);
                first_name = address.first_name;
                last_name = address.last_name;
                street_address_1 = address.address1;
                street_address_2 = address.address2;
                city = address.city;
                province = address.province;
                country = address.country;
                zip = address.zip;
                phone = address.phone;
                full_name = first_name + ' ' + last_name;

                if(street_address_2 === null) {
                    street_address_2 = '';
                }

                shipping_address = street_address_1 + ' ' + street_address_2 + '<br />' + city + ', ' + province + ' ' + zip + '<br />' + country + '<br />' + phone;
                $('#shippingAddress, #shippingAddress2').html(full_name + '<br />' + shipping_address).removeClass('loading');
                $("#addressPaymentLink a, #paymentBillingLink").attr('href', '/tools/recurring/portal/' + wayfx.customerHash + '/payment_sources?address_id=' + address_id + '&token=' + wayfx.customerToken);
            }).fail(function (response) {
                {%- if enable_dev_mode -%}
                console.log(response);
                {%- endif -%}
            });
        }
    }

    {%- else -%}

    function wfxPortalLoad() {
        $(wayfx.upcomingOrder).empty();

        return $.ajax({
            url: '/tools/recurring/portal/' + wayfx.customerHash + '/request_objects?token=' + wayfx.customerToken,
            type: 'get',
            dataType: 'json',
            data: {schema: '{ "orders": { "status": "SUCCESS" }, "settings": {}, "onetimes": [], "subscriptions": {"status": "ACTIVE" } }'}
        }).done(function (response) {
            var allow_onetime = response.settings.customer_portal.onetime.enabled,
                allow_skipping = response.settings.customer_portal.subscription.skip_scheduled_order,
                allow_swapping = response.settings.customer_portal.subscription.change_product,
                allow_discounts = response.settings.customer_portal.discount_input,
                allow_cancellation = response.settings.customer_portal.subscription.cancel_subscription,
                cancellation_minimum_order_count = response.settings.customer_portal.subscription.cancellation_minimum_order_count;

            var subscription_total = 0,
                subscription_products = $.parseJSON(JSON.stringify(_.groupBy(response.subscriptions, ['address_id'])));

            wfxFrequency().done(function (response) {
                var unit = response.products[0].subscription_defaults.order_interval_unit;
                var frequency_options = response.products[0].subscription_defaults.order_interval_frequency_options;

                $('body').attr('data-subscription-unit', unit);

                for (var s = 0; s < frequency_options.length; s++) {
                    var frequency_button = '<button class="wayfx-btn wayfx-btn--primary wayfx-drawer__frequency-' + frequency_options[s] + '" data-frequency="' + frequency_options[s] + '" data-unit="' + unit + '">' + frequency_options[s] + ' ' + unit + '</button>';
                    $(frequency_button).appendTo('.wayfx-drawer__frequency .wayfx-drawer__body');
                }
            });

            $('body').attr('data-allow-onetime', allow_onetime)
                .attr('data-allow-skipping', allow_skipping)
                .attr('data-allow-swapping', allow_swapping)
                .attr('data-allow-discounts', allow_discounts)
                .attr('data-allow-cancellation', allow_cancellation)
                .attr('data-cancellation-minimum', cancellation_minimum_order_count);

            wfxCharges();

            if(allow_swapping) {
                $(wayfx.swapButton).show();
            } else {
                $(wayfx.swapButton).hide();
            }

            if(allow_discounts) {
                $(wayfx.discountBlock).show();
            }

            if(allow_skipping) {
                $(wayfx.skipButton).show();
            }

            if(allow_onetime) {
                $(wayfx.addDropdownWrapper).show();
            } else {
                $(wayfx.addProduct).show();
            }

            $.each(subscription_products, function(key, value) {
                var subscriptionDateChecker = moment(subscription_products[key][0].created_at).isAfter('2020-12-28');

                if(subscriptionDateChecker) {
                    subscription_total++;
                    delivery_date = subscription_products[key][0].next_charge_scheduled_at;
                    delivery_frequency = subscription_products[key][0].order_interval_frequency;
                    order_address_id = key;
                    subscription_item = '<li data-subscription-name="Subscription" data-subscription-frequency="' + delivery_frequency + '" data-subscription-date="'+ delivery_date +'" data-address="' + key + '"><div class="wfx-subscription-date">' + moment(delivery_date).format('MMM Do') + '</div></li>';
                    $(subscription_item).appendTo('#wfx-portal-subscriptions');
                }
            });

