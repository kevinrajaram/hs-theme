    function wfxCancelSubscription(step) {
        closeSidebar();
        if(step === '1') {
            $(".wfx-portal-cancellation-confirmation").hide();
            $(".wayfx-product-cancellation-retention").hide();
            $(".wfx-portal-cancellation-retention").show();
            $("#wfx-portal-cancellation-reasons-list").show();
            $("#wfx-portal-cancellation-popup").show();
        }

        if(step === '2') {
            $(".wfx-portal-cancellation-retention").hide();
            $(".wfx-portal-cancellation-notice").show();
        }

        $(document).on("click", "#wfx_cancel_subscription_rentention", function () {
            $(".wfx-portal-cancellation-notice").hide();
            $(".wfx-portal-cancellation-retention").show();
        });

        if(step === 'final') {
            $(".wfx-portal-cancellation-retention").hide();
            $(".wfx-portal-cancellation-notice").hide();
            $("#wfx-portal-cancellation-popup").hide();
            $(wayfx.updatingAccount).addClass('active');
            setTimeout(function() {
                $('#upcomingOrder .wayfx-product__item').each(function () {
                    subscription_id = $(this).attr('data-subscription-id');
                    var cancellation_reason = $("#wfx-portal-cancellation-reasons-list li.selected .wfx-retention-reason").text();
                    data = {"cancellation_reason": cancellation_reason};
                    $.ajax({
                        url: '/tools/recurring/portal/' + wayfx.customerHash + '/subscriptions/' + subscription_id + '/cancel?token=' + wayfx.customerToken,
                        type: 'post',
                        async: false,
                        dataType: 'json',
                        data: data
                    }).done(function (response) {
                        wayfxAlert("success");
                        {%- if enable_dev_mode -%}
                        console.log(response);
                        {%- endif -%}
                    }).fail(function (response) {
                        wayfxAlert("error");
                        {%- if enable_dev_mode -%}
                        console.log(response);
                        {%- endif -%}
                    });
                    $("#wfx-portal-cancellation-popup").show();
                    $(".wfx-portal-cancellation-confirmation").show();
                    $("#wfx-portal-subscription-notice").hide();
                    $(wayfx.subscriptions).hide();
                    $(wayfx.noSubscriptions).show();
                });
            }, 100);
        }

        $(document).on("click", "#wfx-portal-cancellation-reasons-list li", function () {
            $("#wfx-portal-cancellation-reasons-list li").removeClass('selected');
            $(this).addClass('selected');
            var discount_code = $(this).data('discount'),
                prevention_text = $(this).data('prevention'),
                incentive_type = $(this).data('incentive');

            if(prevention_text) {
                $('.wfx-portal-cancellation-notice .description').html(prevention_text);
                $('.wfx-portal-cancellation-notice .wfx-extra-reasons').hide();
            } else {
                $('.wfx-portal-cancellation-notice .description').html('Are you sure you want to cancel your subscription? You can always skip a shipment instead.');
                $('.wfx-portal-cancellation-notice .wfx-extra-reasons').show();
            }

            if(discount_code) {
                $("#wfx_skip_shipment_popup").hide();
                $("#wfx_add_discount_popup").attr('data-discount', discount_code).show();
            } else {
                $("#wfx_add_discount_popup").hide();
                $("#wfx_skip_shipment_popup").show();
            }

            $("#wfx_cancel_subscription_next").show();
        });

    }

    $(document).on("click", "#wayfx-product-cancellation-reasons-list li", function () {
        $("#wayfx-product-cancellation-reasons-list li").removeClass('selected');
        $(this).addClass('selected');
        $('#wfx_cancel_product_final').show();
    });

    /* Retention */
    wfxRetention().done(function (response) {
        for (var i = 0; i < response.retention_strategies.length; i++) {
            var retention_reason = response.retention_strategies[i].cancellation_reason;
            var retention_prevention = response.retention_strategies[i].prevention_text;
            var retention_incentive = response.retention_strategies[i].incentive_type;
            var retention_discount = response.retention_strategies[i].discount_code;
            var retention_item = '<li data-prevention="'+ retention_prevention.replace(/"/g, "'") +'" data-incentive="' + retention_incentive +'" data-discount="' + retention_discount + '">' +
                '<div class="wfx-retention-reason">'
                + retention_reason +
                '</div>' +
                '</li>';
            $(retention_item).appendTo('#wfx-portal-cancellation-reasons-list');
        }
    }).fail(function (response) {
        {%- if enable_dev_mode -%}
        console.log(response);
        {%- endif -%}
    });

    function addNewProduct(type, d) {
        var url,
            data,
            address_id = addressId('get'),
            delivery_frequency = $("body").attr('data-subscription-frequency'),
            delivery_unit = $("body").attr('data-subscription-unit'),
            delivery_date = $("body").attr('data-subscription-date'),
            delivery_date_formatted = moment(delivery_date).format('YYYY-MM-DD'),
            variant_id = $(d).attr('data-variant-id'),
            quantity = $(d).attr('data-qty');

        if(type === 'one-time') {
            url = '/tools/recurring/portal/' + wayfx.customerHash + '/onetimes?token=' + wayfx.customerToken;
            data = {
                "shopify_variant_id": variant_id,
                "next_charge_scheduled_at": delivery_date_formatted,
                "address_id": address_id,
                "quantity": quantity
            };
        }

        if(type === 'subscription') {
            url = '/tools/recurring/portal/' + wayfx.customerHash + '/subscriptions?token=' + wayfx.customerToken;
            data = {
                "shopify_variant_id": variant_id,
                "order_interval_frequency": delivery_frequency,
                "order_interval_unit": delivery_unit,
                "charge_interval_frequency": delivery_frequency,
                "first_charge_date": delivery_date_formatted,
                "next_charge_scheduled_at": delivery_date_formatted,
                "address_id": address_id,
                "quantity": quantity
            };
        }

        $.ajax({
            url: url,
            type: 'post',
            dataType: 'json',
            data: data
        }).done(function (response) {
            {%- if enable_dev_mode -%}
            console.log(response);
            {%- endif -%}
            wfxSubscription(address_id, "update");
            wayfxAlert("success");
        }).fail(function (response) {
            {%- if enable_dev_mode -%}
            console.log(response);
            {%- endif -%}
        });

    }

    $(document).ready(function () {
        $("#subscriptionLink a").attr('href', '/tools/recurring/portal/' + wayfx.customerHash + '/subscriptions?token=' + wayfx.customerToken);
        $("#ordersLink a").attr('href', '/tools/recurring/portal/' + wayfx.customerHash + '/orders?token=' + wayfx.customerToken);
        wfxPortalLoad();

        {% unless request.path contains 'payment_sources' %}
        wfxBillingDetails();

        $('body').on('change', '.wayfx-product__grid-variants', function () {
            var wfx_selected_variant = $(this).val();
            $(this).parent().parent().parent().parent().find('.wayfx-product__grid-image img').attr('src', productImageJson[0].product_images[wfx_selected_variant]);
            $(this).parent().parent().parent().next('.wfx-add-product-btn').attr('data-variant-id', wfx_selected_variant);
        });

        $(document).on("click", "#wfx-portal-subscription-switcher", function () {
            if($(this).hasClass('active')) {
                $(this).removeClass('active');
                $(this).find('#wfx-portal-subscriptions').fadeOut(100);
            } else {
                $(this).addClass('active');
                $(this).find('#wfx-portal-subscriptions').fadeIn(100);
            }
        });

        $(document).mouseup(function (e) {
            var wfxSidebar = $(".wayfx-drawer");
            var wfxCancelPopup = $("#wfx-portal-cancellation-popup");
            var wfxCancelPopupBlock = $("#wfx-portal-cancellation-popup .block");
            var wfxSubscriptionSwitcher = $('#wfx-portal-subscription-switcher');

            if (!wfxSidebar.is(e.target) && wfxSidebar.has(e.target).length === 0) {
                closeSidebar();
            }

            if (!wfxSubscriptionSwitcher.is(e.target) && wfxSubscriptionSwitcher.has(e.target).length === 0) {
                wfxSubscriptionSwitcher.removeClass('active').find('ul').fadeOut(100);
            }

            if (!wfxCancelPopupBlock.is(e.target) && wfxCancelPopupBlock.has(e.target).length === 0) {
                wfxCancelPopup.hide();
                resetCancelPopup();
            }
        });

        $(document).on("click", ".wayfx-drawer__frequency button", function () {
            closeSidebar();
            $(wayfx.updatingAccount).addClass('active');
            var delivery_frequency = $(this).attr('data-frequency'),
                delivery_unit = $("body").attr('data-subscription-unit'),
                address_id = addressId('get'),
                data = [];

            $('#upcomingOrder .wayfx-product__item').each(function () {
                product_type = $(this).attr('data-product-type');
                if(product_type === 'subscription') {
                    var subscription_id = $(this).attr('data-subscription-id');
                    data.push({
                        "id": subscription_id,
                        "order_interval_unit": delivery_unit,
                        "order_interval_frequency": delivery_frequency,
                        "charge_interval_frequency": delivery_frequency,
                    },);
                }

            });

            var json = { "subscriptions": data };

            wayfxBulkUpdate(address_id, json)
                .done(function(response) {
                    {%- if enable_dev_mode -%}
                    console.log(response);
                    {%- endif -%}
                    $(wayfx.deliveryFrequency).html(delivery_frequency + ' ' + delivery_unit);
                    $("body").attr('data-subscription-frequency', delivery_frequency);
                    $('.wayfx-drawer__frequency button').removeClass('current');
                    $('.wayfx-drawer__frequency-' + delivery_frequency).addClass('current');
                    wayfxAlert("success");
                }).fail(function(response) {
                {%- if enable_dev_mode -%}
                console.log(response);
                {%- endif -%}
            });
        });

        {% endunless %}

    });

    $(document).on("click", ".wayfx-product__item--upcoming", function () {
        var $this = $(this),
            product_type = $this.attr('data-product-type'),
            subscription_id = $this.attr('data-subscription-id'),
            allow_swapping = $('body').attr('data-allow-swapping');


        if(allow_swapping === 'true') {
            if(product_type === 'one-time') {
                var product_onetime_id = $this.attr('data-product-onetime-id');
                $(wayfx.swapButton).hide();
                product_details_container = $(wayfx.drawerProduct).attr('data-subscription-id', subscription_id).attr('data-product-onetime-id', product_onetime_id);
            } else {
                $(wayfx.swapButton).show();
                product_details_container = $(wayfx.drawerProduct).attr('data-subscription-id', subscription_id).attr('data-product-onetime-id', '');
            }
        } else {
            $(wayfx.swapButton).hide();
            product_details_container = $(wayfx.drawerProduct).attr('data-subscription-id', subscription_id);
        }

        product_price = $this.attr('data-product-price');
        product_image_url = $this.attr('data-product-image');
        product_title = $this.attr('data-product-title');
        product_variant_title = $this.attr('data-product-variant-title');
        product_quantity = $this.attr('data-product-quantity');
        product_details_container.empty();
        product_details = '<div class="wayfx-drawer__product-image"><img src="' + product_image_url + '" alt="" /></div>' +
            '<div class="wayfx-drawer__product-title">' + product_title + '</div>' +
            '<div class="wayfx-drawer__product-price">$' + product_price + '</div>';
        $(product_details_container).html(product_details);

        if(allow_swapping === 'true') {
            $(".wayfx-product__swap-item button").attr("data-current-subscription", subscription_id);
            $("#wayfx-drawer__product__swap-current").html('<div class="wayfx-drawer__product-image"><img src="' + product_image_url + '" alt="" /></div>' +
                '<div><div class="wfx-sidebar-swap-title">' + product_title + '</div>' +
                '<div class="wfx-sidebar-swap-price">$' + product_price + '</div></div>');
        }

        $(wayfx.drawerProductQty).val(product_quantity);
        $(wayfx.drawerProductBlock).show();
        $(wayfx.index).toggleClass('wayfx-drawer-active');
        $(wayfx.drawer).toggleClass('active');
    });


    $(document).ready(function () {
        $(wayfx.mobileNavDropdown).click(function () {
            $(wayfx.navLinks).slideToggle();
        });

        $(wayfx.orderHeading).click(function () {
            $(this).toggleClass('active');
            $(this).next(wayfx.orderBody).slideToggle();
        });
    });
