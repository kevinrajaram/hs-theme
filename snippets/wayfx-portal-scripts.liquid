    function delayShipment(days) {
        $("#wfx-portal-cancellation-popup").hide();
        $(wayfx.updatingAccount).addClass('active');

        setTimeout(function() {
            $('#upcomingOrder .wayfx-product__item').each(function () {
                subscription_id = $(this).attr('data-subscription-id');
                var address_id = addressId('get');
                product_next_charge = moment($('body').attr('data-subscription-date')).format("YYYY-MM-DD");
                product_updated_next_charge = moment(product_next_charge).add(days, "days").format("YYYY-MM-DD");
                product_type = $(this).attr('data-product-type');

                if(product_type === 'one-time') {
                    var product_onetime_id = $(this).attr('data-product-onetime-id');
                    data = {
                        "next_charge_scheduled_at": product_updated_next_charge
                    };
                    wayfxSetDate('one-time', data, product_onetime_id);
                } else {
                    data = {
                        "date": product_updated_next_charge
                    };
                    wayfxSetDate('subscription', data, subscription_id);
                }

                var shipmentPicker = $('#nextShipmentCalendar').datepicker({
                    inline: true,
                    language: 'en',
                    minDate: new Date(),
                    toggleSelected: false,
                    onSelect: function onSelect(fd) {
                        current_shipment_date = moment(product_next_charge).format('YYYY-MM-DD');
                        var address_id = addressId('get');

                        if (fd !== current_shipment_date) {
                            closeSidebar();
                            $(wayfx.updatingAccount).addClass('active');
                            var data = [];
                            $('#upcomingOrder .wayfx-product__item').each(function () {
                                subscription_id = $(this).attr('data-subscription-id');
                                product_type = $(this).attr('data-product-type');
                                if(product_type === 'one-time') {
                                    var product_onetime_id = $(this).attr('data-product-onetime-id');
                                    data = {
                                        "next_charge_scheduled_at": fd
                                    };
                                    wayfxSetDate('one-time', data, product_onetime_id, false);
                                } else {
                                    data.push({
                                        "id": subscription_id,
                                        "first_charge_date": fd,
                                        "next_charge_scheduled_at": fd
                                    },);
                                }
                            });
                            var json = { "subscriptions": data };

                            wayfxBulkUpdate(address_id, json)
                                .done(function(response) {
                                    {%- if enable_dev_mode -%}
                                    console.log(response);
                                    {%- endif -%}
                                    $(wayfx.updatingAccount).removeClass('active');
                                    $(wayfx.deliveryDate).html(moment(fd).format('MMM Do'));
                                    $('#wfx-portal-subscriptions li.active .wfx-subscription-date, #wfx-portal-subscription-date').html(moment(fd).format('MMM Do'));
                                    $('body, #wfx-portal-subscriptions li.active').attr('data-subscription-date', fd + 'T00:00:00');
                                    wayfxAlert("success");
                                }).fail(function(response) {
                                {%- if enable_dev_mode -%}
                                console.log(response);
                                {%- endif -%}
                            });
                        }
                    }
                });

                $('.wfx-add-product-btn').attr('data-subscription-date', product_updated_next_charge);
                shipmentPicker.data('datepicker').selectDate(new Date(product_updated_next_charge));
                $(wayfx.deliveryDate).html(moment(product_updated_next_charge).format('MMM Do'));
                return false;
            });
        }, 100);
    }

