    function minusQty(el, d) {
        var $this = $(el),
            qtyTotal = $this.next('.qtyTotal'),
            current_count = qtyTotal.val();

        if (current_count === '1') {
            return;
        }

        var new_count = current_count - 1;
        qtyTotal.val(new_count);

        if(d) {
            $(d).attr('data-qty', new_count);
