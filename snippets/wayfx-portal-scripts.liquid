    function wfxAddDiscount(id, discount_code) {
        return $.ajax({
            url: '/tools/recurring/portal/' + wayfx.customerHash + '/addresses/' + id + '/apply_discount?token=' + wayfx.customerToken,
            type: 'post',
            dataType: 'json',
            data: {
                "discount_code": discount_code
            }
        });
