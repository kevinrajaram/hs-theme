                        <span class="stamped-product-reviews-badge stamped-main-badge" data-id="{{ product.id }}" data-product-sku="{{ product.handle }}" data-product-type="{{product.type}}" data-product-title="{{product.title}}" style="display: inline-block;">{{ product.metafields.stamped.badge }} Reviews</span>
                    </div>
                    <div class="wayfx-product__grid-title">{{ product_title }}</div>
                    <div class="wayfx-product__grid-description">{{ product_description }}</div>
                    {%- comment -%}
                        {%- if product.variants.size > 1 -%}
                            {%- assign has_variants = true -%}
                            <div class="wayfx-product__variant">
                                <div class="wfx-select-wrapper">
                                    <select class="wayfx-product__grid-variants">
                                        {%- for variant in product.variants -%}
                                            <option value="{{ variant.id }}">{{ variant.title | escape }}</option>
                                        {%- endfor -%}
                                    </select>
                                </div>
                            </div>
                        {%- endif -%}
                    
                    <div class="wayfx-product__grid-price">
                        <span class="wayfx-product__grid-original-price">{{ normal_price | money_without_trailing_zeros }}</span>
                        <span class="wayfx-product__grid-discount-price">{{ subscription_price | money_without_trailing_zeros }}</span>
                    </div>
                    {%- endcomment -%}
                </div>
                <div class="wayfx-product__grid-subscribe">
                    <div class="wayfx-product__grid-price">
                        <span class="wayfx-product__grid-original-price">{{ normal_price | money_without_trailing_zeros }}</span>
                        <span class="wayfx-product__grid-discount-price">{{ subscription_price | money_without_trailing_zeros }}</span>
                    </div>
                    <div class="wayfx-product__grid-qty">
                        <div onClick="minusQty(this, '#wayfx-product-{{ normal_variant_id }}')" class="wayfx-btn wayfx-btn--secondary wayfx-btn--outline">
                            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 6 3"><path d="M5.48 2.06H0V0h5.48z" fill="inherit" fill-rule="nonzero"/></svg>
                        </div>
                        <input class="qtyTotal" type="number" name="quantity" min="1" value="1" />
                        <div onClick="addQty(this, '#wayfx-product-{{ normal_variant_id }}')" class="wayfx-btn wayfx-btn--secondary wayfx-btn--outline">
                            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 11 11"><path d="M6.02 0v3.98h4v2.04h-4v4H3.98v-4H0V3.98h3.98V0z" fill="inherit" fill-rule="nonzero"/></svg>
                        </div>
                    </div>
                    <div class="wayfx-product__grid-add-single" style="display: none;">
                        <button onClick="addNewProduct('subscription', '#wayfx-product-{{ normal_variant_id }}')">Add to order</button>
                    </div>
                    <div class="wayfx-product__grid-add-options" style="display: none;">
                        <strong>Add to order</strong>
                        <div class="wayfx-product__grid-add-dropdown" style="display: none;">
                            <ul>
                                <li class="wfx-add-product-onetime-btn" onClick="addNewProduct('one-time', '#wayfx-product-{{ normal_variant_id }}')">One time - {{ normal_price | money_without_trailing_zeros }}</li>
                                <li class="wfx-add-product-btn" style="display: flex; align-items: center; justify-content: space-between;" onClick="addNewProduct('subscription', '#wayfx-product-{{ normal_variant_id }}')">
                                    <strong>Monthly - {{ subscription_price | money_without_trailing_zeros }}</strong>
                                    <span>RECOMMENDED</span>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        {%- endif -%}
    {%- endfor -%}
{%- endcapture -%}

{%- capture swap_products -%}
    {%- for product in collections['all'].products -%}
        {%- assign product_title = product.title -%}
        {%- assign current_variant = product.selected_or_first_available_variant -%}
        {%- assign normal_price = current_variant.price | money_without_trailing_zeros -%}
        {%- assign normal_variant_id = product.variants.first.id -%}
        {%- assign subscription_variant_id = current_variant.id -%}
        {%- assign subscription_id = product.metafields.subscriptions.subscription_id -%}
        {%- assign product_image = product | img_url: 'grande' -%}
        {%- assign subscription_price = current_variant.metafields.subscriptions.discount_variant_price | times: 100 -%}
        {%- if subscription_variant_id and subscription_price > 0 -%}
            <div class="wayfx-product__swap-item">
                <div class="wayfx-product__swap-image">
                    <img src="{{ product_image }}" alt="{{ product.featured_image.alt | escape }}"/>
                </div>
                <div class="wayfx-product__swap-info">
                    <div class="wayfx-product__swap-title">{{ product_title }}</div>
                    <div class="wayfx-product__swap-price">
                        <span class="wayfx-product__swap-discount-price">{{ subscription_price | money }}</span>
                    </div>
                </div>
                <button onClick="swapProduct(this)" class="wayfx-btn wayfx-btn--primary" data-variant-id="{{ normal_variant_id }}">Swap</button>
            </div>
        {%- endif -%}
    {%- endfor -%}
{%- endcapture -%}

{%- capture product_images -%}
    {%- for product in collections['all'].products -%}{%- for variant in product.variants -%}"{{ variant.id }}": "{{ product.featured_image.src | img_url: '400x400' }}",{%- endfor -%}{%- if forloop.last -%}"wayfx": "images"{%- endif -%}{%- endfor -%}
{%- endcapture -%}

