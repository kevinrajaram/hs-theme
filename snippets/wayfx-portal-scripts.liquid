    function wfxRemoveDiscount(id) {
        return $.ajax({
            url: '/tools/recurring/portal/' + wayfx.customerHash + '/addresses/' + id + '/remove_discount?token=' + wayfx.customerToken,
            type: 'post',
            dataType: 'json'
