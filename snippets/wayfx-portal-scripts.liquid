    function wfxSubscription(s_address_id, type) {
        $('#shippingAddress, #shippingAddress2, #deliveryFrequency, #deliveryDate, #upcomingOrder').html('').addClass('loading');

        wayfxOrders(s_address_id).done(function (response) {
            if(type === 'update') {
                $(wayfx.upcomingOrder).empty().removeClass('loading');
            }
            var subscription_products = $.parseJSON(JSON.stringify(_.groupBy(response.subscriptions, ['address_id']))),
                subscription_charges = $.parseJSON(JSON.stringify(_.groupBy(response.charges, ['address_id']))),
                subscription_active_products = subscription_products[s_address_id],
                subscription_queued_charges = subscription_charges[s_address_id][0].shipping_address;

            wfxUpcomingOrder(subscription_active_products, s_address_id);
            wfxShippingAddress(subscription_queued_charges, s_address_id);

            if(subscription_charges[s_address_id][1] !== undefined) {
                var charge = subscription_charges[s_address_id][1];
            } else {
                var charge = subscription_charges[s_address_id][0];
            }

            discount_total = charge.total_discounts;
            before_discount_total = charge.total_line_items_price;

            if(charge.discount_codes.length > 0) {
                discount_code_current = charge.discount_codes[0].code;
                $("body").attr('data-discount-code', discount_code_current);
                $("#wfxDiscountCode").val(discount_code_current);
            }

            $('.wfx-portal-original-total').html('$' + before_discount_total);

            if (discount_total > 0) {
                subscription_total = charge.total_line_items_price - charge.total_discounts;
                $('.wfx-portal-final-total').html('$' + subscription_total.toFixed(2));
                $('.wfx-portal-discount-total').html('You saved $' + discount_total + ' ');
            } else {
                subscription_total = charge.total_price;
                $('.wfx-portal-final-total').html('$' + subscription_total);
                $('.wfx-portal-original-total, .wfx-portal-discount-total').html('');
            }

            $('#upcomingOrder, .wfx-portal-total-price, #wfx-portal-subscription-date').removeClass('loading');

            $(wayfx.deliveryFrequency).html(delivery_frequency + ' Days').removeClass('loading');

            if($("#wfx-portal-subscription-switcher").length > 0) {
                delivery_date = $('#wfx-portal-subscriptions li.active').attr("data-subscription-date");
                delivery_frequency = $('#wfx-portal-subscriptions li.active').attr("data-subscription-frequency");
                $('#wfx-portal-subscription-date').html(moment(delivery_date).format('MMM Do'));
                $(wayfx.deliveryDate).html(moment(delivery_date).format('MMM Do')).removeClass('loading');
            } else {
                $('#wfx-portal-subscription-date').html(delivery_date);
                $(wayfx.deliveryDate).html(delivery_date).removeClass('loading');
            }

            $("#wfx_add_discount_popup").click(function() {
                var discount_to_use = $(this).data('discount');
                $('#wfxDiscountCode').val(discount_to_use);
                $(".wfx-portal-cancellation-notice").hide();
                $("#wfx-portal-cancellation-popup").hide();
                $(".wfx-portal-cancellation-retention").hide();

                $.ajax({
                    url: '/tools/recurring/portal/' + wayfx.customerHash + '/addresses/' + address_id + '/remove_discount?token=' + wayfx.customerToken,
                    type: 'post',
                    dataType: 'json'
                }).done(function (response) {
                    {%- if enable_dev_mode -%}
                    console.log(response);
                    {%- endif -%}

                    $.ajax({
                        url: '/tools/recurring/portal/' + wayfx.customerHash + '/addresses/' + address_id + '/apply_discount?token=' + wayfx.customerToken,
                        type: 'post',
                        dataType: 'json',
                        data: {
                            "discount_code": discount_to_use
                        }
                    }).done(function (response) {
                        {%- if enable_dev_mode -%}
                        console.log(response);
                        {%- endif -%}
                        wayfxAlert("success");
                    }).fail(function (response) {
                        $('.wfx-discount-error').html(' Discount not found.');
                    });
                });

            });
        }).fail(function (response) {
            $(wayfx.subscriptions).hide();
            $(wayfx.updatingAccount).removeClass('active');
            $(wayfx.noSubscriptions).show();
            {%- if enable_dev_mode -%}
            console.log(response);
            {%- endif -%}
        });
    }

